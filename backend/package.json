{
  "name": "uol-backend",
  "version": "1.0.0",
  "description": "app para gerenciamento de cadastro de clientes",
  "main": "index.js",
  "scripts": {
    "db:reset": "npx prisma migrate reset",
    "db:init": "npx prisma migrate dev --name init",
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run build && npm run db:reset",
    "start": "node ./build/server.js",
    "predev": "npm run db:init",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
    "generate-schema": "npx prisma generate",
    "test": "jest"
  },
  "keywords": [],
  "author": "vania ioott",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.11.7",
    "@types/validator": "13.6.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.0.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "arg": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1"
  },
  "prisma": {
    "seed": "ts-node seed.ts"
  }
}
