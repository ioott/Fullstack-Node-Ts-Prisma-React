{
  "name": "backend",
  "version": "1.0.0",
  "description": "app para gerenciamento de empresa de organização de eventos",
  "main": "index.js",
  "scripts": {
    "db:reset": "npx prisma migrate reset --force",
    "db:init": "npx prisma migrate dev --name init",
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run build && npm run db:reset",
    "start": "node ./build/server.js",
    "predev": "npm run db:init",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/api/server.ts",
    "generate-schema": "npx prisma generate",
    "test": "jest"
  },
  "keywords": [],
  "author": "vania ioott",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.11.7",
    "@types/validator": "13.6.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.0.0",
    "prettier": "3.2.5",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "arg": "^5.0.2",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "uuid": "^9.0.1"
  }
}
